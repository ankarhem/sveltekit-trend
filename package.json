		"@neoconfetti/svelte": "^1.0.0",
		"@rgossiaux/svelte-headlessui": "^1.0.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@types/cookie": "^0.5.1",
		"@types/node": "^18.11.9",
