		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.2.4",
		"ts-node": "^10.9.1",
		"tslib": "^2.3.1",
		"typescript": "^4.9.3",
		"vite": "^3.1.0",
